    requirement = 'The system shall be able to model the diffusion of T2, H2 and HT across a membrane separating two enclosures in accordance with Sieverts’ law with a concentration jump at the interface and generate an exodus file.'
  []
  [ver-1kc-2_comparison]
    type = RunCommand
    command = 'python3 comparison_ver-1kc-2.py'
